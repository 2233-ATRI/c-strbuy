#include<stdio.h>
int main()
{
   int a,b,c;
    b=0;
   int x,y,z;//百十个位 
   scanf("%d",&a);
   for(int i=1;i<1000;i++)
   {
           c=i;
        x=c/100;
        y=c%100/10;
        z=c%100%10;
        if(x+y+z==a)
        { printf("%8d",i);
                 b++;
        if(b%6==0)
         printf("\n");}
     } 
    // printf("\n");
     return 0;
}
